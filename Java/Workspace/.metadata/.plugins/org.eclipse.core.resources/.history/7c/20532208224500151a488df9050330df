
public class graphs {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Node c1 = new Node(); 
		Node c2 = new Node(); 
		Node c3 = new Node(); 
		Node c4 = new Node(); 
		Edge e1 = new Edge(c1, c2, 5);
		Edge e2 = new Edge(c2, c3, 10);
		Edge e3 = new Edge(c3, c4, 15);
		Edge e4 = new Edge(c4, c1, 20);
		
		
	}
	public class Node{
		Node next, prev;
		Node(){
			this.next = null;
			this.prev = null;
		}
	}
	public class Edge{
		Node c1, c2;
		int weight;
		Edge(Node con1, Node con2, int w){
			this.c1 = con1;
			this.c2 = con2;
			this.weight = w;
		}
		
		
	}

}
